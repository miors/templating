  <%- include('partials/head') %>
  <body>
    <%- include('partials/navbar') %>
    <h1>Your random number is: <%= rand %></h1>
    <% if(rand % 2 === 0) { %>
    <h1>This is an even number</h1>
    <% } else { %>
    <h1>This is an odd number</h1>
    <% } %>
    <h3><%= rand%2 === 0? 'EVEN' : 'ODD' %></h3>
  </body>
</html>
